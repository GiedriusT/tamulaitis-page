{
  "name": "tamulaitis-page",
  "homepage": "https://tamulaitis.lt",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "start": "pnpm dev",
    "check": "rm -r dist || astro check",
    "build": "pnpm check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint ./src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "pnpm lint --fix",
    "test": "pnpm playwright test",
    "test:ui": "pnpm playwright test --ui",
    "generate:md": "pnpm dlx ts-node --esm --experimental-specifier-resolution=node src/utils/mdgen.ts",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "update-interactive": "pnpm update --latest --interactive",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "astro": "^4.16.16",
    "dotenv": "^16.4.5",
    "highlight.js": "^11.9.0",
    "marked": "^13.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-transition-group": "^4.4.5",
    "rehype-raw": "^7.0.0",
    "sass": "^1.77.5",
    "unist-util-visit": "^5.0.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "^3.6.3",
    "@astrojs/sitemap": "^3.2.1",
    "@playwright/test": "^1.44.1",
    "@types/node": "18",
    "@types/react": "^18.3.3",
    "@types/react-transition-group": "^4.4.10",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "typescript": "^5.4.5"
  },
  "pnpm": {
    "overrides": {
      "rollup": "npm:@rollup/wasm-node"
    }
  }
}
