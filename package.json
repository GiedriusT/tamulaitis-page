{
  "name": "tamulaitis-page",
  "homepage": "https://tamulaitis.lt",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "pnpm playwright test",
    "test:ui": "pnpm playwright test --ui",
    "generate:md": "npx ts-node --esm --experimental-specifier-resolution=node src/utils/mdgen.ts",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "update-interactive": "pnpm update --latest --interactive",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "is-touch-device": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-helmet-async": "^1.3.0",
    "react-markdown": "^9.0.0",
    "react-router-dom": "^6.14.2",
    "react-transition-group": "^4.4.5",
    "rehype-raw": "^7.0.0",
    "styled-components": "^6.0.5",
    "styled-normalize": "^8.0.7",
    "unist-util-visit": "^5.0.0",
    "uuid": "^9.0.0",
    "web-vitals": "^3.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.39.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^29.5.3",
    "@types/node": "18.18.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-transition-group": "^4.4.6",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.2",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.52.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-playwright": "^0.18.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^5.9.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.6"
